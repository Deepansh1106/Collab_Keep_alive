{
  "manifest_version": 3,
  "name": "Colab Keep Alive (Experimental)",
  "version": "1.0",
  "description": "Experimental extension to simulate activity in Google Colab.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": [
    "https://colab.research.google.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
  "16": "logo.png",
  "32": "logo.png",
  "48": "logo.png",
  "128": "logo.png"
}
}
